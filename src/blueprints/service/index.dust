{#imports}
import {.} from './{.}'
{/imports}

@Injectable
export default class {name} {
    constructor(http: Http) {};

{@each key=rpc}
    {$key|firstLetterInLowerCase|camelCase}(request: {$value.request}): Observable<{$value.response}> {
        {#$value.options}
        return this.http.post('{post}', request.{body})
            .map(res => res.json())
        {/$value.options}
    };

{/each}
}
